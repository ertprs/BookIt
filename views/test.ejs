    <script>
        function nextslides() {
            var right = document.getElementById("right-div");
            var left = document.getElementById("left-div");
            var count = $('#right').data('counter');
            if (count >= 1) {
                left.classList.remove('displayNone');
            }
            if (count > 3) {
                right.classList.add('displayNone');
            }


            var d = count + 1;


            var first1 = document.getElementById("dt" + count);

            var first2 = document.getElementById("dt" + d);

            first1.classList.add('displayNone');
            first1.style.transition = "all 10s delay";
            first2.style.transition = "all 10s delay";
            $('#right').data('counter', d);


        }

        function prevslides() {

            var right = document.getElementById("right-div");
            var left = document.getElementById("left-div");
            var count = $('#right').data('counter');
            if (count < 2) {
                left.classList.add('displayNone');
            }
            console.log(count);
            var c1 = count - 1;
            var c2 = count - 1;


            var d = count + 5;


            var prev1 = document.getElementById("dt" + c1);


            var prev2 = document.getElementById("dt" + 4);
            prev2.classList.add('displayNone');
            prev1.classList.remove('displayNone');
            if (c2 <= 4) {
                right.classList.remove('displayNone');
            }

            // first1.style.transition = "all 10s delay";
            // first2.style.transition = "all 10s delay";
            $('#right').data('counter', count - 1);


        }
    </script>
