                                                                                                    <% } %>
                                                                                                        <% if(doctor.department== "Dentist"){ %>

                                                                                                            <img src="/img/specialities/teeth.png" class="img-fluid" alt="Speciality">
                                                                                                            <% } %>
                                                                                                                <%=doctor.department%>

                                    </p>
                                    <%var sum=0,avg,cnt=doctor.reviews.length;%>
                                    <%for(review of doctor.reviews){%>
                                        <%sum+=review.rating;%>
                                    <%}%>
                                    <%avg=sum/cnt;console.log(avg,sum)%>
                                    <div class="rating">
                                        <%if(avg>=1){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=2){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=3){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=4){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=5){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <span class="d-inline-block average-rating">(<%=doctor.reviews.length%>)</span>
                                    </div>

