                                                <ul class="experience-list">
                                                    <% for(ssr of doctor.awards){ %>
                                                        <li>
                                                            <div class="experience-user">
                                                                <div class="before-circle"></div>
                                                            </div>
                                                            <div class="experience-content">
                                                                <div class="timeline-content">
                                                                    <p class="exp-year">
                                                                        <%=ssr.year %>
                                                                    </p>
                                                                    <h4 class="exp-title">
                                                                        <%=ssr.award  %>
                                                                    </h4>

                                                                </div>
                                                            </div>
                                                        </li>
                                                        <% } %>


                                                </ul>
                                            </div>
                                        </div>
                                        <!-- /Awards Details -->

                                        <!-- Services List -->
                                        <div class="service-list">
                                            <h4>Services</h4>

                                            <ul class="clearfix">
