                                        <%}%>
                                   
                                </div>
                                <div class="clinic-booking">
                                    <a class="apt-btn" href="/booking/?id=<%=doctor._id%>">Book Appointment</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /Doctor Widget -->

                <!-- Doctor Details Tab -->
                <div class="card" >
                    <div class="card-body pt-0">

                        <!-- Tab Menu -->
