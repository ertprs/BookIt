                                    <%var sum=0,avg,cnt=doctor.reviews.length;%>
                                    <%for(review of doctor.reviews){%>
                                        <%sum+=review.rating;%>
                                    <%}%>
                                    <%avg=sum/cnt;console.log(avg,sum)%>
                                    <div class="rating text-center">
                                        <%if(avg>=1){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=2){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
                                        <%if(avg>=3){%>
                                        <i class="fas fa-star filled"></i>
                                        <%}else{%>
                                            <i class="fas fa-star"></i>
                                        <%}%>
