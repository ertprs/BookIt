                                        <div class="service-list">
                                            <h4>Specializations</h4>
                                            <ul class="clearfix">
                                                <%if(doctor.specialisation){%>

                                                    <%var service = doctor.specialisation.toString().split(',')%>
                                                        <%for(temp of service){%>
                                                            <li>
                                                                <%=temp%>
                                                            </li>
                                                            <%}}%>
                                            </ul>
                                        </div>
                                        <!-- /Specializations List -->

                                    </div>
                                </div>
                            </div>
                            <!-- /Overview Content -->

                            <!-- Locations Content -->
                            <div role="tabpanel" id="doc_locations" class="tab-pane fade">

                                <!-- Location List -->
                                <div class="location-list" style="padding: 5px !important;"> 
                                    <div class="row">

                                        <!-- Clinic Content -->
                                        <div class="col-md-4" style="padding-left: 0px !important;padding-right: 0px !important;">
