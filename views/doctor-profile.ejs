                                        <div class="col-md-2">
                                            <div class="consult-price">
                                                Rs
                                                <%=doctor.booking_fee%>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- /Location List -->



                            </div>
                            <!-- /Locations Content -->

