                                    <p class="doc-speciality">
                                        <% for(degrees of doctor.education){ %>
                                            <%=degrees.degree.toUpperCase() %>&nbsp&nbsp&nbsp
                                                <% } %>
                                    </p>
                                    <p class="doc-department">
                                        <% if(doctor.department== "General Physician"){ %>

                                            <img src="/img/specialities/physical.jfif" class="img-fluid" alt="Speciality">
                                            <% } %>

                                                <% if(doctor.department== "Ophthalmology"){ %>

                                                    <img src="/img/specialities/eye.png" class="img-fluid" alt="Speciality">
                                                    <% } %>
                                                        <% if(doctor.department== "Dermatology"){ %>

                                                            <img src="/img/specialities/hair.jpg" class="img-fluid" alt="Speciality">
                                                            <% } %>
                                                                <% if(doctor.department== "Paediatrics"){ %>

                                                                    <img src="/img/specialities/child.jpg" class="img-fluid" alt="Speciality">
                                                                    <% } %>
                                                                        <% if(doctor.department== "Urology"){ %>

                                                                            <img src="/img/specialities/brain.png" class="img-fluid" alt="Speciality">
                                                                            <% } %>
                                                                                <% if(doctor.department== "Neurology"){ %>

                                                                                    <img src="/img/specialities/specialities-03.png" class="img-fluid" alt="Speciality">
