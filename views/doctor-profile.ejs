                                            <%var service = doctor.services.toString().split(',')%>
                                                <%for(temp of service){%>
                                                    <span><%=temp%></span>
                                                    <%}}%>

                                    </div>
                                </div>
                            </div>
                            <div class="doc-info-right">
                                <div class="clini-infos">
                                    <ul>
                                       
                                        <li><i class="fas fa-map-marker-alt"></i>
                                            <%=doctor.contacts.city%>
                                                <%=doctor.contacts.state%>
                                        </li>
                                        <li><i class="far fa-money-bill-alt"></i>RS
                                            <%=doctor.booking_fee%>
                                        </li>
                                    </ul>
                                </div>
                                <div class="doctor-action">
                                    <%var flag = false;%>
                                                <%if(locals.user){%>
                                                    <%for(fav of locals.user.favourites){
                                                    
                                                   
                                                if(fav.toString() == doctor._id)flag = true;%>

                                                        <%}%>
                                                            <%}%>

                                                                <%if(!flag){%>
