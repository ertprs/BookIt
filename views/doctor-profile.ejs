                                                                <%for(let i = 1;i<time.start.length;i++){%>
                                                                   </br>
                                                                    <%}%>
                                                            

                                                            <%}else{%>
                                                               

                                                            <%}%>

                                                        <%}%>
                                                    </p>
                                                    <p class="timings-times">
                                                        <%for(time of doctor.schedule_time){%>
                                                            <%if(typeof(time.start) == 'object'){%>

